<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <skos:ConceptScheme xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity">
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="en">Degree of conformity</dc:title>
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="de">Grad der Übereinstimmung</dc:title>
    <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/" xml:lang="fr">Niveau de conformité</dc:title>
    <dcterms:issued xmlns:dcterms="http://purl.org/dc/terms/">2021-06-18</dcterms:issued>
  </skos:ConceptScheme>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity/conformant">
    <skos:prefLabel xml:lang="en">Conformant</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The resource is fully conformant with the cited specification.</skos:scopeNote>
    <skos:prefLabel xml:lang="de">Konform</skos:prefLabel>
    <skos:scopeNote xml:lang="de">Die Ressource stimmt mit der angegebenen Spezifikation in vollem Umfang überein.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Conforme</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">La ressource est totalement conforme à la spécification indiquée.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity" />
  </skos:Concept>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity/notConformant">
    <skos:prefLabel xml:lang="en">Not Conformant</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The resource does not conform to the cited specification.</skos:scopeNote>
    <skos:prefLabel xml:lang="de">Nicht konform</skos:prefLabel>
    <skos:scopeNote xml:lang="de">Die Ressource stimmt mit der angegebenen Spezifikation nicht überein.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Non conforme</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">La ressource n’est pas conforme à la spécification indiquée.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity" />
  </skos:Concept>
  <skos:Concept xmlns:skos="http://www.w3.org/2004/02/skos/core#" rdf:about="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity/notEvaluated">
    <skos:prefLabel xml:lang="en">Not evaluated</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Conformance has not been evaluated.</skos:scopeNote>
    <skos:prefLabel xml:lang="de">Nicht überprüft</skos:prefLabel>
    <skos:scopeNote xml:lang="de">Die Übereinstimmung ist nicht überprüft worden.</skos:scopeNote>
    <skos:prefLabel xml:lang="fr">Non évaluée</skos:prefLabel>
    <skos:scopeNote xml:lang="fr">La conformité de la ressource n’a pas été évaluée.</skos:scopeNote>
    <skos:inScheme rdf:resource="http://inspire.ec.europa.eu/metadata-codelist/DegreeOfConformity" />
  </skos:Concept>
</rdf:RDF>

